{
  "name": "@npmcli/lint",
  "version": "1.0.2",
  "main": "lib/index.js",
  "bin": {
    "lint": "lib/index.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "description": "lint the npmcli way",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/npm/lint"
  },
  "author": "GitHub Inc.",
  "license": "ISC",
  "scripts": {
    "test": "tap",
    "snap": "tap",
    "preversion": "npm test",
    "postversion": "npm publish",
    "prepublishOnly": "git push origin --follow-tags",
    "npmclilint": "node .",
    "lint": "eslint '**/*.js'",
    "lintfix": "npm run lint -- --fix",
    "posttest": "npm run lint",
    "postsnap": "npm run lintfix --",
    "postlint": "npm-template-check"
  },
  "tap": {
    "check-coverage": true
  },
  "devDependencies": {
    "@npmcli/template-oss": "^2.1.0",
    "tap": "^15.0.9"
  },
  "dependencies": {
    "@npmcli/eslint-config": "^1.0.2",
    "which": "^2.0.2"
  },
  "templateVersion": "2.1.0",
  "engines": {
    "node": "^12.13.0 || ^14.15.0 || >=16"
  }
}
